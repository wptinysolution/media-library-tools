{
	"name": "media-edit",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:mamunur105/media-edit.git",
	"author": "Mamunur Rashid <rmamunur105@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production && npm run cleanmap",
		"production": "mix --production",
		"package": "npm run clean && npm run dev && npm run prod && npm run dev --package",
		"clean": "rm -rf tmp/*",
		"cleanmap": "rm -rf assets/**/*.map",
		"zip": "npm run package && npm run dev --zip"
	},
	"dependencies": {
		"@ant-design/icons": "^5.0.1",
		"antd": "^5.1.6",
		"axios": "^1.2.2",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.3.0",
		"react-sortable-hoc": "^2.0.0",
		"react-table": "^7.8.0",
		"toastr": "^2.1.4"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.18.6",
		"archiver": "^5.3.1",
		"cli-color": "^2.0.3",
		"emojic": "^1.1.17",
		"fs-extra": "^10.1.0",
		"laravel-mix": "^6.0.49",
		"postcss": "^8.3.1",
		"sass": "^1.54.0",
		"sass-loader": "^12.1.0",
		"wp-pot": "^1.10.2"
	}
}
