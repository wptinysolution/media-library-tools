{
	"name": "tsmlt-media-tools",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:mamunur105/media-edit.git",
	"author": "Mamunur Rashid <rmamunur105@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production && npm run cleanmap",
		"production": "mix --production",
		"package": "npm run clean && npm run dev && npm run prod && cross-env npm_config_package=true npm run dev",
		"clean": "rm -rf dist/*",
		"cleanmap": "rm -rf assets/**/**/*.map",
		"zip": "npm run package && cross-env npm_config_zip=true npm run dev"
	},
	"dependencies": {
		"@ant-design/icons": "^5.3.7",
		"antd": "^5.20.5",
		"axios": "1.12.0",
		"papaparse": "^5.5.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.23.1",
		"toastr": "^2.1.4"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.24.6",
		"archiver": "^6.0.2",
		"cli-color": "^2.0.4",
		"cross-env": "^7.0.3",
		"emojic": "^1.1.17",
		"fs-extra": "^11.2.0",
		"laravel-mix": "^6.0.49",
		"postcss": "^8.4.38",
		"sass": "^1.77.2",
		"sass-loader": "^14.2.1",
		"wp-pot": "^1.10.2"
	}
}
